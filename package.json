{
  "name": "node-img-placeholder",
  "version": "0.0.0-semantically-released",
  "description": "run a local image placeholder service",
  "main": "lib/index.js",
  "bin": "bin/www",
  "scripts": {
    "commit": "git-cz",
    "report-coverage": "cat coverage/lcov.info | codecov",
    "start": "node bin/www",
    "test": "istanbul cover -x *.test.js _mocha -- -R spec ./tests/*.test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "keywords": [
    "node local image placeholder service",
    "node image placeholder",
    "image placeholder",
    "image placeholder service",
    "image generator",
    "placeholder",
    "node",
    "local",
    "image",
    "placeholder",
    "service"
  ],
  "engines": {
    "node": ">= 5.2.x"
  },
  "bugs": {
    "url": "https://github.com/ifahrentholz/lips/issues"
  },
  "author": "Ingo Fahrentholz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ifahrentholz/lips"
  },
  "dependencies": {
    "express": "^4.13.3",
    "gm": "^1.21.1",
    "morgan": "^1.6.1",
    "node.extend": "^1.1.5",
    "serve-favicon": "^2.3.0"
  },
  "devDependencies": {
    "chai": "3.4.1",
    "codecov.io": "^0.1.6",
    "cz-conventional-changelog": "^1.1.5",
    "ghooks": "^1.0.3",
    "istanbul": "^0.4.2",
    "jade": "^1.11.0",
    "mocha": "2.3.4",
    "semantic-release": "^4.3.5",
    "supertest": "1.1.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test"
    }
  }
}
